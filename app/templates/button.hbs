<a {{action "clickButton" button}}>{{button.title}}
{{#if button.status}}
	<span class="buttonStatus">{{button.status}}</span>
{{/if}}
</a>
{{#if controller.displayStatuses}}
	{{#each possibleStatus in button.statuses}}
		<ul>
			<a {{action "selectStatus" button possibleStatus}}>{{possibleStatus}}</a>
		</ul>
	{{/each}}
{{/if}}